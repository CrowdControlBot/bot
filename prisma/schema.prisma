generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model cc {
  id String @id @default(dbgenerated()) @map("_id") @db.ObjectId
}

model Guild {
  id String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  guildID String @unique()
  premium Boolean
  settings Json?
}